{
  "name": "@nlxchat/widget",
  "version": "2.0.0-alpha.12",
  "description": "Standalone chat widget based on the NLX Chat SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "parcel serve -p 8081",
    "build:website": "parcel build --public-url ./",
    "build:umd": "node bin/build.js",
    "build": "rm -rf lib && mkdir lib && tsc && npm run build:umd",
    "prepublish": "npm run build"
  },
  "targets": {
    "website": {
      "source": "src/website/index.html"
    }
  },
  "author": "Peter Szerzo <peter@nlx.ai>",
  "license": "MIT",
  "devDependencies": {
    "@types/marked": "^4.0.8",
    "@types/node": "^11.15.11",
    "@types/ramda": "^0.26.44",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/tinycolor2": "^1.4.2",
    "browserify": "^17.0.0",
    "htm": "^3.1.1",
    "parcel": ">=2.9.3",
    "prettier": "^1.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsify": "^5.0.4",
    "typescript": "^5.2.2",
    "uglifyify": "^5.0.2"
  },
  "dependencies": {
    "@emotion/cache": "^11.1.3",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@nlxchat/core": "^2.0.0-alpha.12",
    "@nlxchat/react": "^2.0.0-alpha.12",
    "marked": "^4.3.0",
    "promise-polyfill": "^8.1.3",
    "ramda": "^0.27.1",
    "tinycolor2": "^1.4.1",
    "whatwg-fetch": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
